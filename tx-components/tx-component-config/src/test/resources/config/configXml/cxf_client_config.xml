<?xml version="1.0" encoding="UTF-8"?>
<configProperties>
	<property key="crm.helloworldservice.url" name="cxf客户端配置">
		<property key="testCRMconfig" name="测试crm系统配置">
			<property key="helloWorldServiceConfig" name="HelloWorldSerivce配置">
				<property key="hellowordservice.url" name="hellowordservice.url">
					<defaultValue>http://localhost:8080/HelloWorldService</defaultValue>
					<developValue>http://localhost:8081/HelloWorldService</developValue>
				</property>
				<property key="crm.helloworldservice.timeout" name="hellowordservice.timeout">
					<defaultValue>2000</defaultValue>
					<description>单位毫秒,小于或等于0，表示不限制超时时间</description>
				</property>
			</property>
		</property>
	</property>
</configProperties>